{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "definitions": {
    "Division": {
      "additionalProperties": false,
      "properties": {
        "area": {
          "description": "Area in km².",
          "type": "number"
        },
        "highlights": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "image": {
          "type": "string"
        },
        "imageAttribution": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "population": {
          "type": "number"
        },
        "type": {
          "description": "The list of all divisions should cover all land without overlap. Where appropriate, to keep\nthings intuitive, some divisions may be grouped together (e.g. just 제주 which covers 제주시 and\n서귀포시).",
          "enum": ["광역시", "군", "시", "특별시", "특별자치도", "특별자치시"],
          "type": "string"
        }
      },
      "required": ["area", "highlights", "image", "name", "population", "type"],
      "type": "object"
    }
  },
  "properties": {
    "$schema": {
      "type": "string"
    },
    "divisions": {
      "items": {
        "$ref": "#/definitions/Division"
      },
      "type": "array"
    }
  },
  "required": ["divisions"],
  "type": "object"
}
